<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CheckableImageCardView">
        <attr name="checkable" format="boolean"/>
        <attr name="isChecked" format="boolean"/>
        <attr name="aspectRatio" format="float"/>
        <attr name="fixedDimension" format="enum">
            <enum name="isWidth" value="0"/>
            <enum name="isHeight" value="1"/>
        </attr>
    </declare-styleable>
</resources>
